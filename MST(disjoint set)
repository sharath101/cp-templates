#include <bits/stdc++.h>
using namespace std;
typedef long long int ll;
#define pb push_back
#define mp make_pair
#define deb(x) cout<< #x << " " << x << "\n";
const ll mod=1e9+7;
 
ll par[10001];
     
ll find(ll a){
    if(par[a]<0)               //only 1 indexing
        return a;
    return par[a]=find(par[a]);
}
ll unionn(ll a,ll b){
	if(find(a)==find(b)) return;
    if(par[find(a)]>par[find(b)])
        par[find(a)]=find(b);
    else
        par[find(b)]=find(a);
}
     
int main() {
        
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
     
    ll n,m;
    cin>>n>>m;
    ll u,v,c;
    vector<pair<ll,pair<ll,ll>>> a;
    for(ll i=0;i<m;i++){
        cin>>u>>v>>c;
        a.pb(mp(c,mp(u,v)));
    }
    sort(a.begin(),a.end());
    for(ll i=1;i<=n;i++) par[i]=-1*i;
    ll ans=0;
    for(ll i=0;i<a.size();i++){
        ll u=a[i].second.first,v=a[i].second.second;
        if(find(u)==find(v))
            continue;
        else
            unionn(u,v);
        ans+=a[i].first;
    }
    cout<<ans;
 
    return 0;
}
