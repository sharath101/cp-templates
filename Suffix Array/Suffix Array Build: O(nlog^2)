#include <bits/stdc++.h>
using namespace std;
#define ll long long int
#define pb push_back
#define mp make_pair
#define deb(x) cout<< #x << " " << x << "\n";
#define MAX 9223372036854775807
#define MIN -9223372036854775807
#define PI 3.141592653589
#define setbits(n) __builtin_popcountll(n)
#define mkunique(a) a.resize(unique(a.begin(),a.end())-a.begin());
const ll mod=1e9+7;


void buildsa(string s){
    s+="$";
    ll n=s.size();
    vector<pair<char,ll>> a;
    for(ll i=0;i<n;i++) a.pb({s[i],i});
        sort(a.begin(),a.end());
    vector<ll> p(n);
    for(ll i=0;i<n;i++) p[i]=a[i].second;
    vector<ll> c(n,0);
    for(ll i=1;i<n;i++){
        if(a[i].first!=a[i-1].first)
            c[p[i]]=c[p[i-1]]+1;
        else
            c[p[i]]=c[p[i-1]];
    }
    ll k=0;
    while((1<<k)<n){
        vector<pair<pair<ll,ll>,ll>> a(n);
        for(ll i=0;i<n;i++)
            a[i]={{c[i],c[i+(1<<k)]},i};
        sort(a.begin(),a.end());
        for(ll i=0;i<n;i++) p[i]=a[i].second;
        c[p[0]]=0;
        for(ll i=1;i<n;i++){
            if(a[i].first!=a[i-1].first)
                c[p[i]]=c[p[i-1]]+1;
            else
                c[p[i]]=c[p[i-1]];
        }
        k++;
    }
    suff=p;
}


int main() {
    
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    cout.tie(NULL);
    ll T=clock();


    string s;
    cin>>s;
    vector<ll> sa = buildsa(s);

    for(ll i=0;i<sa.size();i++)
        cout<<sa[i]<<" ";




    cerr<<"\n\nTIME: "<<(double)(clock()-T)/CLOCKS_PER_SEC<<" sec\n";
    T = clock();
    return 0;
}
