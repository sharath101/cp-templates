int dfs(ll v){
    pos[v]=s.size();
    s.pb(v);
    ll ok=-1;
    for(ll u:a[v]){
        if(pos[u]!=-1 && pos[v]-pos[u]>1)
            ok=max(ok,pos[u]);
    }
    if(ok!=-1){
        for(ll i=ok;i<=pos[v];i++)
            cyc.pb(s[i]);
        return 1;
    }
    for(ll u:a[v]){
        if(pos[u]==-1){
            if(dfs(u)==1)
                return 1;
        }
    }
    s.pop_back();
    return 0;
}
